Xcursor.theme:    Vanilla-DMZ-AA

!urxvt color scheme:

*background: #2B2B2B
*foreground: #DEDEDE

*colorUL: #86a2b0

! black
*color0  : #2E3436
*color8  : #555753
! red
*color1  : #CC0000
*color9  : #EF2929
! green
*color2  : #4E9A06
*color10 : #8AE234
! yellow
*color3  : #C4A000
*color11 : #FCE94F
! blue
*color4  : #3465A4
*color12 : #729FCF
! magenta
*color5  : #75507B
*color13 : #AD7FA8
! cyan
*color6  : #06989A
*color14 : #34E2E2
! white
*color7  : #D3D7CF
*color15 : #EEEEEC

!urxvt fonts: 

*font: xft:Monospace:pixelsize=12
*boldFont: xft:Monospace:bold:pixelsize=12


!urxvt extensions:

!URxvt.perl-ext-common:  default,matcher,tabbedex

!enabling clickable links:

*.urlLauncher:      /usr/bin/firefox
*.matcher.button:   1 

!urxvt scrolling options and cursor style:

*saveLines: 12000
*scrollstyle:plain
*scrollBar: false
*cursorBlink: true
*cursorUnderline: true

! urxvt tabs settings:

*.tabbed.tabbar-fg: 2
*.tabbed.tabbar-bg: 0
*.tabbed.tab-fg: 2
*.tabbed.tab-bg: 8
*.tabbed.autohide:  yes
*.tabbed.new-button:  no
*.tabbed.title: no
